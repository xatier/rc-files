{
    "C_Cpp.default.cStandard": "c18",
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.intelliSenseMode": "clang-x64",
    "C_Cpp.updateChannel": "Insiders",
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.find.autoFindInSelection": "always",
    "editor.fontSize": 16,
    "editor.renderControlCharacters": true,
    "editor.renderWhitespace": "all",
    "editor.suggest.localityBonus": true,
    "editor.suggest.maxVisibleSuggestions": 15,
    "editor.tabCompletion": "on",
    "editor.rulers": [
        79,
        119
    ],
    "files.exclude": {
        "**/*.o": true,
        "**/*.so*": true,
        "**/*.pyc": true
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "go.autocompleteUnimportedPackages": true,
    "go.useLanguageServer": true,
    "python.analysis.completeFunctionParens": true,
    "python.analysis.diagnosticMode": "workspace",
    "python.analysis.logLevel": "Warning",
    "python.analysis.memory.keepLibraryAst": true,
    "python.analysis.symbolsHierarchyDepthLimit": 20,
    "python.analysis.typeCheckingMode": "strict",
    "python.autoComplete.addBrackets": true,
    "python.formatting.provider": "yapf",
    "python.formatting.yapfArgs": [
        "--style",
        "{dedent_closing_brackets: true, split_before_logical_operator: false, split_complex_comprehension: true}"
    ],
    "python.insidersChannel": "weekly",
    "python.jediEnabled": false,
    "python.languageServer": "Microsoft",
    "python.linting.banditEnabled": true,
    "python.linting.enabled": true,
    "python.linting.flake8Args": [
        "--ignore C408,D1",
        "--import-order-style=google"
    ],
    "python.linting.flake8Enabled": true,
    "python.linting.mypyArgs": [
        "--follow-imports=silent",
        "--ignore-missing-imports",
        "--no-strict-optional",
        "--show-column-numbers"
    ],
    "python.linting.mypyEnabled": true,
    "python.pythonPath": "${workspaceRoot}/venv/bin/python",
    "python.testing.nosetestsEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": false,
    "search.collapseResults": "alwaysExpand",
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "vim.foldfix": true,
    "vim.gdefault": true,
    "vim.highlightedyank.enable": true,
    "vim.hlsearch": true,
    "vim.vimrc.enable": true,
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "vsintellicode.features.python.deepLearning": "enabled",
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.colorTheme": "GitHub Dark",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.titleScrollbarSizing": "large",
    "workbench.iconTheme": "vscode-icons",
    "xo.enable": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
